{
  "name": "@hai3/api",
  "version": "0.1.0-alpha.0",
  "description": "API communication protocols and service registry for HAI3 SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "CLAUDE.md",
    "llms.txt",
    "commands"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "axios": "^1.6.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "keywords": [
    "hai3",
    "api",
    "rest",
    "sse",
    "typescript"
  ],
  "author": "HAI3",
  "license": "Apache-2.0"
}
