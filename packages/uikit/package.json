{
  "name": "@hai3/uikit",
  "version": "0.1.0-alpha.7",
  "description": "HAI3 UI Kit - Reusable React components built on shadcn/ui",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "src/styles/**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./src/styles/globals.css"
  },
  "files": [
    "dist",
    "src/styles",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "hai3",
    "react",
    "components",
    "ui-kit",
    "shadcn",
    "tailwind"
  ],
  "author": "HAI3org",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/HAI3org/HAI3.git",
    "directory": "packages/uikit"
  },
  "bugs": {
    "url": "https://github.com/HAI3org/HAI3/issues"
  },
  "homepage": "https://github.com/HAI3org/HAI3#readme",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@hai3/uikit-contracts": "*",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.2",
