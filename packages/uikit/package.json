    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.0.0",
    "input-otp": "^1.4.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "react-day-picker": "^9.12.0",
    "react-hook-form": "^7.68.0",
    "react-resizable-panels": "^3.0.6",
