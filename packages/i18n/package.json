{
  "name": "@hai3/i18n",
  "version": "0.1.0-alpha.0",
  "description": "Internationalization and translation management for HAI3 SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "CLAUDE.md",
    "llms.txt"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {},
  "keywords": [
    "hai3",
    "i18n",
    "internationalization",
    "localization",
    "typescript"
  ],
  "author": "HAI3",
  "license": "Apache-2.0"
}
